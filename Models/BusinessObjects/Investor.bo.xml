        <Operation Name="HasPermission" Description="" RuleType="Default" EventName="" ApplyToAttribute="" BaseInfo="ApplicationSystemBO:ApplicationUser" IsStatic="false" IsInherited="true" IsExternal="false">function bool HasPermission(string permission)
{
  bool hasPermissionfromRoles = this.Roles.Any(rr =&gt; rr.Permissions.Any(pp =&gt; pp.Name == permission));
	return hasPermissionfromRoles || this.Permissions.Any(pp =&gt; pp.Name == permission);
}            </Operation>
        <Operation Name="GetDomainFocusOptions" Description="" RuleType="Default" EventName="" ApplyToAttribute="" BaseInfo="" IsStatic="true" IsInherited="false" IsExternal="false">static function Collection[string] GetDomainFocusOptions(Domain.Investor inv) 
{
	Collection[string] options;
	
	options.Add("Recurring");
	options.Add("Project-based");
	options.Add("Other");
	
	if (!options.Contains(inv.DomainFocus) &amp;&amp; inv.DomainFocus != null) {
		options.Add(inv.DomainFocus);
	}
	
	return options;
}</Operation>
