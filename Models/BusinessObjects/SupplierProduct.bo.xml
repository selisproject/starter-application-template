        <Operation Name="HasPermission" Description="" RuleType="Default" EventName="" ApplyToAttribute="" BaseInfo="ApplicationSystemBO:ApplicationUser" IsStatic="false" IsInherited="true" IsExternal="true">function bool HasPermission(string permission)
{
  bool hasPermissionfromRoles = this.Roles.Any(rr =&gt; rr.Permissions.Any(pp =&gt; pp.Name == permission));
	return hasPermissionfromRoles || this.Permissions.Any(pp =&gt; pp.Name == permission);
}            </Operation>
        <Operation Name="IsSupplier" Description="" RuleType="Default" EventName="" ApplyToAttribute="" BaseInfo="" IsStatic="false" IsInherited="false" IsExternal="true">function bool IsSupplier()
{
//	Domain.Supplier sup = Domain.Supplier.Find(a=&gt; a.UserName == userName).First();
	if (this.IsInRole("GuestSupplier") ||
		this.IsInRole("PendingRegistered") ||
		this.IsInRole("RegisteredSupplier") ||
		this.IsInRole("PendingValidated") ||
		this.IsInRole("ValidatedSupplier") ||
		this.IsInRole("ActiveSupplier")) {
