        <Operation Name="IsSupplier" Description="" RuleType="Default" EventName="" ApplyToAttribute="" BaseInfo="" IsStatic="false" IsInherited="false" IsExternal="true">function bool IsSupplier(string userName)
{
	Domain.Supplier sup = Domain.Supplier.Find(a=&gt; a.UserName == userName).First();
	if (sup.IsInRole("GuestSupplier") ||
		sup.IsInRole("PendingRegistered") ||
		sup.IsInRole("RegisteredSupplier") ||
		sup.IsInRole("PendingValidated") ||
		sup.IsInRole("ValidatedSupplier") ||
		sup.IsInRole("ActiveSupplier")) {
	    return true;
	}
	return false;
}</Operation>
