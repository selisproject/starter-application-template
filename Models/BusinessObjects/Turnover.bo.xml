			.Find(a=&gt; a.Supplier == supplier)
			.Select(a=&gt; a.TransactionDateOccured.Year())
			.Distinct()
			.OrderBy(a=&gt;a.ToString() );
	
	foreach int: year in years {
		Domain.TurnoverStatistic stat;
		stat.Supplier = supplier;
		stat.TotalAmount =  transactions.Where(a=&gt; a.TransactionDateOccured.Year() == year).Sum(a=&gt;a.TransactionValue);
		stat.Year = year;
		stat.TotalAmountFunded = transactions.Where(a=&gt;a.TransactionDateOccured.Year() == year).Sum(a=&gt;a.FundedAmount);
		
		
		stats.Add(stat);
	}
	
	return stats;
}</Operation>
        <Operation Name="GetTurnoverStatisticsPerMonth" Description="" RuleType="Default" EventName="" ApplyToAttribute="" BaseInfo="" IsStatic="true" IsInherited="false" IsExternal="false">static function Collection[Domain.TurnoverStatistic] GetTurnoverStatisticsPerMonth(Domain.Supplier supplier, int year)
{
 	Collection[Domain.TurnoverStatistic] stats;
 	Collection[Domain.Transaction] transactions;
 	Collection[int] months;
    try{
	   	transactions = Domain.Transaction.Find(a=&gt; a.Supplier == supplier &amp;&amp; a.TransactionDateOccured.Year() == year);
		months = Domain.Transaction
				.Find(a=&gt; a.Supplier == supplier)
				.Select(a=&gt;a.TransactionDateOccured.Month())
				.Distinct();
		months = months.OrderBy(a=&gt;a);
		foreach int: month in months) {
			Domain.TurnoverStatistic stat;
			stat.Supplier = supplier;
			stat.TotalAmount =  transactions.Where(a=&gt; a.TransactionDateOccured.Month() == month).Sum(a=&gt;a.TransactionValue);
			stat.TotalAmountFunded = transactions.Where(a=&gt; a.TransactionDateOccured.Month() == month).Sum(a=&gt; a.FundedAmount);
			stat.Month = month;
			stats.Add(stat);
		}
	
    }
    catch Exception e {
        DebugLib.Logger.WriteErrorLine("Error" + e.Message);
        DebugLib.Logger.WriteErrorLine("OH-OH!!!!" + transactions.Count(a=&gt;a!=null) + "--------------"+ months.Count(a=&gt;a!=null));
    }

	
	return stats;

}</Operation>
